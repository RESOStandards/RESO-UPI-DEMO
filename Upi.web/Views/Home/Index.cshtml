@{
    ViewData["Title"] = "Home Page";
}

<div layout="row" layout-sm="column">
    <div flex>
        <md-card>
            <md-card-content>
                <h2>UPI Workgroup</h2>
                <a href="http://members.reso.org/display/PUID">UPI Home</a>
            </md-card-content>
            <md-card-footer>

            </md-card-footer>
        </md-card>
    </div>
    <div flex>
        <md-card>
            <md-card-content>
                <h2>ANSI/FIPS</h2>
                <a href="https://www.census.gov/geo/reference/codes/cou.html">Counties</a>
                <a href="https://www.census.gov/prod/techdoc/cbp/95-96cd/fips-st.pdf">Counties (PDF)</a>
                
            </md-card-content>
            <md-card-footer>

            </md-card-footer>
        </md-card>
    </div>
    <div flex>
        <md-card-header>
            <md-card-header>
                <md-card-avatar>
                    <img class="md-user-avatar" src="https://www.census.gov/main/responsive-header/images/census-logo.png">
                </md-card-avatar>
                <md-card-header-text>
                    <h3>ANSI/FIPS</h3>
                </md-card-header-text>
            </md-card-header>
            <md-card-content>
                <a href="https://www.census.gov/geo/reference/codes/cousub.html">SubCounties</a>
            </md-card-content>
            <md-card-footer>

            </md-card-footer>
        </md-card-header>
    </div>
</div>





<md-card>
    <md-card-header>
        <md-card-avatar>
            <img class="md-user-avatar" src="https://www.census.gov/main/responsive-header/images/census-logo.png">
        </md-card-avatar>
        <md-card-header-text>
            <span class="md-title">Title</span>
            <span class="md-subhead">Sub header</span>
        </md-card-header-text>
    </md-card-header>
    <img ng-src="https://www.census.gov/main/responsive-header/images/census-logo.png" class="md-card-image" alt="image caption">
    <md-card-title>
        <md-card-title-text>
            <span class="md-headline">Card headline</span>
            <span class="md-subhead">Card subheader</span>
        </md-card-title-text>
    </md-card-title>
    <md-card-actions layout="row" layout-align="start center">
        <md-button>Action 1</md-button>
        <md-button>Action 2</md-button>
        <md-card-icon-actions>
            <md-button class="md-icon-button" aria-label="icon">
                <md-icon md-svg-icon="icon"></md-icon>
            </md-button>
        </md-card-icon-actions>
    </md-card-actions>
    <md-card-content>
        <p>
            Card content
        </p>
    </md-card-content>
</md-card>